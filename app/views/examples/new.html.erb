<h1>Add new example</h1>

    <%= form_for @example do |form| %>
      <div>
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div>
      <%= form.label :description %>
      <%= form.text_area :description %>
    </div>
    <div>
      <%= form.label :url %>
      <%= form.text_field :url %>
    </div>
    <%= form.submit "Create" %>

  <% end %>

